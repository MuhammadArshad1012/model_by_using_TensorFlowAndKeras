        "# define vocabluary\n",
        "vocabluary=len(tokenizer.word_index)\n",
        "print(\"number of unique words : \",vocabluary)\n",
        "output_length=le.classes_.shape[0]\n",
        "print(\"output_length : \",output_length)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "s2EisQszFEt9"
      },
      "outputs": [],
      "source": [
        "# creating the model\n",
        "from tensorflow.keras.layers import Input, Embedding, LSTM, Dense, Flatten\n",
        "from tensorflow.keras.models import Model\n",
        "\n",
        "# Define input_shape\n",
        "input_shape = x_train.shape[1]  # This is the length of your padded sequences\n",
        "\n",
        "# Creating the model\n",
        "i = Input(shape=(input_shape,))\n",
        "x = Embedding(vocabluary + 1, 10)(i)\n",
        "x = LSTM(10, return_sequences=True)(x)\n",
        "x = Flatten()(x)\n",
        "x = Dense(output_length, activation=\"softmax\")(x)\n",
        "model = Model(i, x)\n",
        "\n"
      ]
    },
    {
